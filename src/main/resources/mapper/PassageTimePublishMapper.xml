<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.serein.mapper.PassageTimePublishMapper">

    <resultMap id="BaseResultMap" type="com.serein.model.entity.PassageTimePublish">
            <result property="passageId" column="passageId" jdbcType="BIGINT"/>
            <result property="publishTime" column="publishTime" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        passageId,publishTime
    </sql>
  <select id="scanPublishPassage" resultType="com.serein.model.entity.PassageTimePublish">
    select * from blog.passage_time_publish where publishTime >= #{minutesAgoDate}
  </select>
</mapper>
